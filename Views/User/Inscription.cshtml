@model IEnumerable<MVC_ASP_EVENT.Models.EventTypeDay>

@{
    ViewData["Title"] = "Inscription";
}

<h1>Inscription</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
             
                @Html.ActionLink("inscription", "inscriptionDay","User", new {  id=item.EventId, date = item.date })
            </td>
        </tr>
}
    </tbody>
</table>
